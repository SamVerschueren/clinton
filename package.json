{
  "name": "clinton",
  "version": "0.0.0",
  "description": "Project style linter",
  "license": "MIT",
  "repository": "SamVerschueren/clinton",
  "author": {
    "name": "Sam Verschueren",
    "email": "sam.verschueren@gmail.com",
    "url": "github.com/SamVerschueren"
  },
  "main": "index.js",
  "bin": "cli.js",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "test": "xo && ava"
  },
  "files": [
    "index.js",
    "cli.js",
    "rules",
    "lib"
  ],
  "keywords": [
    "github",
    "gh",
    "lint",
    "linter",
    "package",
    "pkg",
    "awesome"
  ],
  "dependencies": {
    "chalk": "^1.1.3",
    "escape-string-regexp": "^1.0.5",
    "gh-got": "^4.0.0",
    "globby": "^4.0.0",
    "got": "^6.2.0",
    "is-d": "^1.0.0",
    "mem": "^0.1.0",
    "meow": "^3.7.0",
    "path-exists": "^2.1.0",
    "pify": "^2.3.0",
    "semver": "^5.1.0",
    "spdx-license-list": "^2.1.0",
    "string-occurrence": "^1.1.1",
    "travis-got": "^1.0.0",
    "z-schema": "^3.17.0"
  },
  "devDependencies": {
    "ava": "*",
    "execa": "^0.4.0",
    "xo": "*"
  },
  "xo": {
    "esnext": true
  },
  "clinton": {
    "rules": {
      "use-travis": "error",
      "license": [
        "error",
        "MIT"
      ],
      "pkg-files": "error",
      "valid-version": "error",
      "pkg-schema": "error",
      "pkg-property-order": "error"
    }
  }
}
